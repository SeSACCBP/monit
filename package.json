{
  "name": "monit",
  "version": "1.0.0",
  "description": "monit homepage",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server" : "nodemon index.js",
    "client" : "npm start --prefix client",
    "heroku-postbuild": "cross-env NODE_PATH=src npm run dev",
    "dev": "concurrently \"npm run server \" \"npm run client \""
  },
  "keywords": [
    "monit"
  ],
  "author": "buksango",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "nodemailer": "^6.9.0",
    "nodemon": "^2.0.20"
  },
  "engines": {
    "node": "19.3.0",
    "npm": "9.2.0"
  }
}
